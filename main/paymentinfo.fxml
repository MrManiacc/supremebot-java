<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<?import java.net.URL?>
<VBox prefHeight="400.0" prefWidth="640.0" xmlns="http://javafx.com/javafx/8.0.65" xmlns:fx="http://javafx.com/fxml/1"
      fx:controller="controllers.AddPaymentInfoController">
    <stylesheets>
        <URL value="@BasicApplication.css" />
    </stylesheets>
  <children>
    <AnchorPane maxHeight="-1.0" maxWidth="-1.0" prefWidth="473.0" VBox.vgrow="ALWAYS">
      <children>
            <TextField fx:id="name" layoutX="237.0" layoutY="14.0" promptText="name" />
            <TextField fx:id="email" layoutX="237.0" layoutY="41.0" promptText="email" />
            <TextField fx:id="tel" layoutX="237.0" layoutY="68.0" promptText="tel" />
            <TextField fx:id="address" layoutX="237.0" layoutY="95.0" promptText="address" />
            <TextField fx:id="zip" layoutX="237.0" layoutY="122.0" promptText="zip" />
            <TextField fx:id="city" layoutX="237.0" layoutY="149.0" promptText="city" />
            <TextField fx:id="state" layoutX="237.0" layoutY="175.0" promptText="state" />
            <TextField fx:id="country" layoutX="237.0" layoutY="202.0" promptText="country" />
            <TextField fx:id="cardType" layoutX="237.0" layoutY="229.0" promptText="card type" />
            <TextField fx:id="number" layoutX="237.0" layoutY="256.0" promptText="number" />
            <TextField fx:id="expMonth" layoutX="237.0" layoutY="283.0" prefHeight="27.0" prefWidth="83.5" promptText="exp. month" />
            <TextField fx:id="expYear" layoutX="321.0" layoutY="283.0" prefHeight="27.0" prefWidth="83.5" promptText="exp. year" />
            <TextField fx:id="cvv" layoutX="237.0" layoutY="310.0" promptText="CVV" />
            <Button layoutX="261.0" layoutY="348.0" mnemonicParsing="false" text="Cancel" />
            <Button layoutX="321.0" layoutY="348.0" mnemonicParsing="false" text="Submit" onAction="#confirmPaymentInfo" />
      </children>
    </AnchorPane>
  </children>
</VBox>
